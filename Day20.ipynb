        "# Min array elements unique\n",
        "def uni(arr):\n",
        "  arr.sort()\n",
        "\n",
        "  res = 0\n",
        "\n",
        "  for i in range(1,len(arr)):\n",
        "    if arr[i] <= arr[i-1]:\n",
        "      inc = arr[i-1] - arr[i] + 1\n",
        "      arr[i] = arr[i-1] + 1\n",
        "      res += inc\n",
        "  return res\n",
        "print(uni([3, 2, 1, 2, 1, 7]))"
      ]
    }
  ]
}